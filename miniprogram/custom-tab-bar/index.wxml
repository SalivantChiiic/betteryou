<view class="tab-bar">
	<block wx:for="{{list}}" wx:key="pagePath">
		<view class="tab-bar-item" data-path="{{item.pagePath}}" data-index="{{index}}" bindtap="switchTab">
			<image class="tab-bar-image" src="{{selected == index ? item.selectedIconPath : item.iconPath}}"></image>
		</view>
	</block>
</view>